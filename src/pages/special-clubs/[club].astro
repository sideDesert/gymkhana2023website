---
export function getStaticPaths(){
    return [
        {params: {club: 'elan'}},
        {params: {club: 'e-cell'}},
        {params: {club: 'tedex'}},
        {params: {club: 'sunshine'}},
        {params: {club: 'milan'}},
        {params: {club: 'FCC'}},
    ]
}
import Layout from "../../layouts/Layout.astro";
const {club} = Astro.params
const title = {
    'elan': 'Elan & nVision',
    'e-cell': 'E-Cell',
    'tedex' : 'Tedx IIT Hyderabad',
    'sunshine': 'Sunshine',
    'milan': 'Milan',
    'FCC': 'Finance and Consulting Club'
}

import MobileView from "../../components/special clubs/MobileView";
---

<Layout title = {title[club]}>
    <MobileView client:visible/>
</Layout>